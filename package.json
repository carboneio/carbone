{
  "name": "carbone",
  "description": "Fast, Simple and Powerful report generator. Injects JSON and produces PDF, DOCX, XLSX, ODT, PPTX, ODS, ...!",
  "version": "2.0.0-enterprise-edition",
  "bin": "bin/carbone",
  "main": "./lib",
  "publishConfig": {
    "registry": "https://npm.ideolys.com"
  },
  "author": "David Grelaud",
  "license": "Carbone-Enterprise-Edition",
  "homepage": "https://carbone.io",
  "keywords": [
    "report",
    "document",
    "pdf",
    "xslx",
    "docx",
    "odt",
    "ods",
    "json"
  ],
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha test --timeout 100000 --exit",
    "lint": "eslint ./*/**.js",
    "lint:fix": "eslint ./*/**.js --fix",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag $PACKAGE_VERSION && git push origin --tags && git push github-origin --tags"
  },
  "dependencies": {
    "debug": "=4.1.1",
    "image-size": "=0.7.4",
    "moment": "=2.19.3",
    "moxie-zip": "=0.0.3",
    "simple-get": "^4.0.0",
    "timsort": "=0.3.0",
    "which": "=2.0.2",
    "yauzl": "=2.10.0"
  },
  "devDependencies": {
    "eslint": "=7.3.1",
    "mocha": "=3.5.3",
    "mocha-sinon": "^2.1.2",
    "nock": "=12.0.0",
    "sinon": "^9.0.2",
    "xml-beautifier": "0.4.3",
    "zipfile": "=0.5.11"
  },
  "engines": {
    "node": ">= 10.15.0"
  }
}
