{
  "name": "carbone-ee",
  "description": "Fast, Simple and Powerful report generator. Injects JSON and produces PDF, DOCX, XLSX, ODT, PPTX, ODS, ...!",
  "version": "4.0.0-beta.3",
  "bin": "bin/carbone",
  "main": "./lib",
  "publishConfig": {
    "registry": "https://npm.ideolys.com"
  },
  "author": "David Grelaud",
  "private": true,
  "carbonePublicationDate": "2022-06-15T05:55:45.124Z",
  "license": "Carbone-Enterprise-Edition",
  "homepage": "https://carbone.io",
  "keywords": [
    "report",
    "document",
    "pdf",
    "xslx",
    "docx",
    "odt",
    "ods",
    "json"
  ],
  "scripts": {
    "test": "./node_modules/.bin/mocha test --timeout 100000 --exit",
    "lint": "eslint ./lib/**.js ./test/**.js ./formatters/**.js",
    "lint:fix": "eslint ./lib/**.js ./test/**.js ./formatters/**.js --fix",
    "build": "pkg -o ./build/carbone-ee .",
    "publishBuild": "script/publishBuild.sh",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag ee-$PACKAGE_VERSION && git push origin ee-$PACKAGE_VERSION"
  },
  "dependencies": {
    "body-parser": "=1.19.0",
    "bwip-js": "=3.0.4",
    "cors": "=2.8.5",
    "dayjs": "=1.10.7",
    "dayjs-timezone-iana-plugin": "=0.1.0",
    "debug": "=4.3.2",
    "echarts": "5.3.1",
    "formidable": "=2.0.1",
    "image-size": "=1.0.1",
    "kitten-jwt": "=1.1.1",
    "restana": "=4.9.2",
    "simple-get": "=4.0.1",
    "which": "=2.0.2",
    "yauzl": "=2.10.0",
    "yazl": "=2.5.1"
  },
  "pkg": {
    "assets": [
      "lib/converter.py",
      "bin/install.sh",
      "bin/systemd",
      "examples/healthCheck.odt",
      "studio/**/*"
    ]
  },
  "devDependencies": {
    "form-data": "=4.0.0",
    "mocha": "=9.1.3",
    "nock": "=13.2.1",
    "pdfjs-dist": "=2.10.377",
    "sinon": "=12.0.1",
    "xml-beautifier": "=0.5.0"
  },
  "engines": {
    "node": ">= 12.14.0"
  }
}
